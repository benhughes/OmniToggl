{
  "defaultLocale": "en",
  "author": "oshiire",
  "identifier": "com.github.oshiire.OmniToggl",
  "version": "1.2.0",
  "description": "A collection of actions for starting Toggl from OmniFocus. See: https://github.com/sho7650/OmniToggl",
